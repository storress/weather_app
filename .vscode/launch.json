    "version": "0.2.0",
    "configurations": [
    
        {
            "name": "weather_app",
            "request": "launch",
            "type": "dart",
            "toolArgs": [
                "--dart-define=OPENWEATHER_API_KEY=API KEY"
            ]
            
        },
        {
            "name": "weather_app (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "toolArgs": [
                "--dart-define=OPENWEATHER_API_KEY=API KEY"
            ]
        },
        {
            "name": "weather_app (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "toolArgs": [
                "--dart-define=OPENWEATHER_API_KEY=API KEY"
            ]
        }
    ]
}